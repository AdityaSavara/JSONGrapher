{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/datum"
      }
    },
    "layout": {
      "$ref": "#/definitions/layout"
    }
  },
  "definitions": {
    "datum": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "uid": {
          "type": "string"
        },
        "comments": {
          "type": "string"
        },
        "line": {
          "$ref": "#/definitions/Line"
        },
        "name": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "x": {
          "type": "array",
          "items": {}
        },
        "y": {
          "type": "array",
          "items": {}
        }
      },
      "required": ["name", "type", "x", "y"],
      "title": "Datum"
    },
    "layout": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "title": {
          "type": "string"
        },
        "xaxis": {
          "$ref": "#/definitions/Xaxis"
        },
        "yaxis": {
          "$ref": "#/definitions/Yaxis"
        },
        "legend": {
          "$ref": "#/definitions/Legend"
        },
        "autosize": {
          "type": "boolean"
        },
        "plot_bgcolor": {
          "type": "string"
        },
        "paper_bgcolor": {
          "type": "string"
        }
      },
      "required": ["title", "xaxis", "yaxis"],
      "title": "Layout"
    },
    "Line": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "shape": {
          "type": "string"
        },
        "width": {
          "type": "integer"
        }
      },
      "title": "Line"
    },
    "Legend": {
      "type": "object",
      "additionalProperties": true,
      "title": "Legend"
    },
    "Xaxis": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "title": {
          "type": "string"
        },
        "showgrid": {
          "type": "boolean"
        },
        "autorange": {
          "type": "boolean"
        }
      },
      "required": ["title"],
      "title": "Xaxis"
    },
    "Yaxis": {
      "type": "object",
      "additionalProperties": true,
      "properties": {
        "title": {
          "type": "string"
        },
        "autorange": {
          "type": "boolean"
        },
        "gridcolor": {
          "type": "string"
        },
        "ticksuffix": {
          "type": "string"
        }
      },
      "required": ["title"],
      "title": "Yaxis"
    }
  },
  "required": ["title", "data", "layout"],
  "additionalProperties": true
}
