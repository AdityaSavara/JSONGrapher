                }
            },
            "required": [
                "data",
                "layout",
                "title"
            ],
            "title": "requiredProperties"
        },
        "Datum": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
                "uid": {
                    "type": "string"
                },
                "line": {
                    "$ref": "#/definitions/Line"
                },
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "x": {
                    "type": "array",
                    "items": {
                    }
                },
                "y": {
                    "type": "array",
                    "items": {
                    }
                }
