{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://kineticmodels.edu/transient_mass_fraction.schema.json",
  "title": "Transient mass concentration",
  "description": "Evolution of the mass fraction of a compound during a chemical reaction",
  "type": "object",
  "properties": {
    "compoundName": {
      "description": "The standard name of compound",
      "type": "string"
    },
    "compoundFormula": {
      "description": "Chemical formulat of the compound",
      "type": "string"
    },
    "time": {
      "description": "Time elapsed since the start of the reactor",
      "type": "array",
      "items": {
        "type": "number"
      },
      "minItems": 1,
    },
    "timeUnit": {
      "description": "Unit used to measure time",
      "type": "string"
    },
    "massFraction": {
      "description": "Mass fraction of the compound in the reactor",
      "type": "array",
      "items": {
        "type": "number"
      },
      "minItems": 1,
    },
    "massFractionUnit": {
      "description": "Unit used to quantity the mass fraction",
      "type": "string"
    }
  },
  "required": [ "compoundName", "compoundFormula", "time", "timeUnit", "massFraction", "massFractionUnit" ]
}